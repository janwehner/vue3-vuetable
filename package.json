{
  "name": "vue3-vuetable",
  "version": "3.0.4",
  "description": "Datatable component for Vue 3.x",
  "main": "dist/vue3-vuetable.js",
  "license": "MIT",
  "author": "Rati Wannapanop <rati.wannapanop@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janwehner/vue3-vuetable.git"
  },
  "private": false,
  "keywords": [
    "vue",
    "component",
    "table",
    "datatable",
    "pagination-components",
    "json"
  ],
  "browserify": {
    "transform": [
      "babelify",
      [
        "vueify",
        {
          "_flags": {
            "debug": true
          }
        }
      ]
    ]
  },
  "scripts": {
    "serve": "webpack serve --config build/webpack.dev.conf.js --hot --progress",
    "build": "cross-env MINIFY=true webpack --progress --config build/webpack.prod.conf.js",
    "test": "yarn jest"
  },
  "dependencies": {
    "axios": "^0.27"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@vue/compiler-sfc": "^3.3",
    "@vue/test-utils": "^2.4",
    "@vue/vue3-jest": "^29.2",
    "babel-jest": "^29.7",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "html-webpack-plugin": "^5.3.1",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^1.5.0",
    "moxios": "^0.4.0",
    "promise-polyfill": "^8.2.0",
    "terser-webpack-plugin": "^5.1.1",
    "typescript": "^4.5.4",
    "vue": "^3.3",
    "vue-loader": "^17.2",
    "vue-style-loader": "4.1.3",
    "webpack": "^5.35.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "5.7.3"
  },
  "engines": {
    "node": ">= 14.16.0",
    "npm": ">= 6.14.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/janwehner/vue3-vuetable/issues"
  },
  "homepage": "https://github.com/janwehner/vue3-vuetable#readme",
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
